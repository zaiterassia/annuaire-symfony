{% extends 'base.html.twig' %}

{% block title %}Seo{% endblock %}

{% block body %}
    <h1>Seo</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ seo.id }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ seo.url }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ seo.username }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ seo.password }}</td>
            </tr>
            <tr>
                <th>Response</th>
                <td>{{ seo.response }}</td>
            </tr>
            <tr>
                <th>ResponseUrl</th>
                <td>{{ seo.responseUrl }}</td>
            </tr>
            <tr>
                <th>Observations</th>
                <td>{{ seo.observations }}</td>
            </tr>
            <tr>
                <th>EditDate</th>
                <td>{{ seo.editDate ? seo.editDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_seo_index') }}">back to list</a>

    <a href="{{ path('app_seo_edit', {'id': seo.id}) }}">edit</a>

    {{ include('seo/_delete_form.html.twig') }}
{% endblock %}
